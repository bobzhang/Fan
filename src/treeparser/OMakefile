

# These files depend on the interfaces in ../lib
OCAMLINCLUDES += $(COMMON_INCLUDES)
OCAMLFLAGS =  -w a 
FILES[]=
    gaction
    gcomb
    gdelete	
    gfailed	
    ginsert	
    gparser
    gprint
    gstru
    gstructure
    gtools

OCamlLibrary(libtreeparser, $(FILES))

.PHONY: clean
clean:
    rm -rf $(OCAMLBIN_EXT)
