

OCAMLINCLUDES += +compiler-libs
OCAMLINCLUDES += $(UTILS_INCLUDES)
LocalFiles=oconfig.ml
$(LocalFiles): oconfig.mlp
	sed -e "s|%%LIBDIR%%|$(OCAMLLIB)|" $< > $@

FILES =  ast2pt oconfig astPrint ast_basic  configf\
    locf  fcset fdir foptions\
    tokenf lex_util lexgen location_util parsetree_util\
    tokenEval translate_lex fan_util \
    fan_warnings
    
.PHONY: clean lib
lib: $(OCamlLibrary libcommon, $(FILES))


clean:
    rm -rf $(OCAMLBIN_EXT)
    rm -rf $(LocalFiles)


