

FILES = fchar fstring fstream return listf dyn_array fan_warnings mapf setf hashset \
    filenamef hashtblf ref stackf option bufferf arrayf formatf int util

lib: $(OCamlLibrary libutils, $(FILES))


.PHONY: clean lib
clean:
    rm -rf $(OCAMLBIN_EXT)

